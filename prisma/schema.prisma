datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
  moduleFormat = "cjs"
}

model User {
  id    Int     @default(autoincrement()) @id
  email String  @unique
  name  String?
  createdAt DateTime @default(now())
}

model Carrera {
  id        Int      @default(autoincrement()) @id
  title     String
  course    String[]
  createdAt DateTime @default(now())
}